bp-install gmp
bp-install mpfr
bp-install mpc
download https://bigsearcher.com/mirrors/gcc/releases/gcc-4.9.4/gcc-4.9.4.tar.gz
extract
./configure $(configure-flags \
	--with-gmp=%HOME%/.backpack/pkg/gmp \
	--with-mpfr=%HOME%/.backpack/pkg/mpfr \
	--with-mpc=%HOME%/.backpack/pkg/mpc)
make -j
make -j install
link-to-root
